{
  "name": "type-type-music-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@uiw/react-codemirror": "^3.2.1",
    "axios": "^0.27.2",
    "codemirror": "^5.65.3",
    "firebase": "^9.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.4",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.4.2",
    "tone": "^14.7.77",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "con": "concurrently \"cd ../type-type-music-backend && npm run nodemon\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "semi": [
        "error",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "semi-style": [
        "warn",
        "last"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-multi-spaces": "warn",
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "space-infix-ops": "warn",
      "comma-spacing": "warn",
      "block-spacing": "warn",
      "semi-spacing": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "key-spacing": "warn",
      "eol-last": "warn",
      "no-undef": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "(useRecoilCallback|useRecoilTransaction_UNSTABLE)"
        }
      ]
    },
    "plugins": [
      "react-hooks"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "concurrently": "^7.2.2",
    "coverage": "^0.4.1",
    "react-recoil-hooks-testing-library": "^0.1.0"
  }
}
